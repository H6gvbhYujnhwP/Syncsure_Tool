{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "LicenseKey": "",
  "MaxDevices": 50,
  "Api": {
    "BaseUrl": "https://syncsure-backend.onrender.com",
    "TimeoutSeconds": 30,
    "RetryAttempts": 3,
    "RetryDelaySeconds": 5
  },
  "Agent": {
    "HeartbeatMinutes": 5,
    "UpdateCheckHours": 24,
    "LogRetentionDays": 14,
    "EnableAutoUpdate": true,
    "EnableOneDriveProbes": true
  }
}

